        "yiisoft/yii2-gii":"2.0.5"
    },
    "config": {
        "process-timeout": 1800
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject",
            "Proseeds\\CodeSniffer\\Installer::setupStandards"
        ],
        "post-install-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS",
            "Codeception\\c3\\Installer::copyC3ToRoot",
            "Proseeds\\CodeSniffer\\Installer::setupStandards",
            "find ./web/assets/* ! -name '.gitignore' | xargs rm -rf"
        ],
        "post-update-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS",
            "Codeception\\c3\\Installer::copyC3ToRoot",
            "Proseeds\\CodeSniffer\\Installer::setupStandards",
            "find ./web/assets/* ! -name '.gitignore' | xargs rm -rf"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ],
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/proseeds/proseeds"
        },
        {
            "type": "vcs",
            "url": "https://github.com/proseeds/code-sniffer"
        }
    ]
}
